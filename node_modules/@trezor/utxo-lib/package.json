    "prepublishOnly": "yarn tsx ../../scripts/prepublishNPM.js",
    "prepublish": "yarn tsx ../../scripts/prepublish.js"
  },
  "dependencies": {
    "@trezor/utils": "9.2.6",
    "bchaddrjs": "^0.5.2",
    "bech32": "^2.0.0",
    "bip66": "^2.0.0",
    "bitcoin-ops": "^1.4.1",
    "blake-hash": "^2.0.0",
    "blakejs": "^1.2.1",
    "bn.js": "^5.2.1",
    "bs58": "^6.0.0",
    "bs58check": "^4.0.0",
    "create-hmac": "^1.1.7",
    "int64-buffer": "^1.0.1",
    "pushdata-bitcoin": "^1.0.1",
    "tiny-secp256k1": "^1.1.6",
    "typeforce": "^1.18.0",
    "varuint-bitcoin": "2.0.0",
    "wif": "^5.0.0"
